{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --port 8080 --host '0.0.0.0'",
    "build": "run-p build-only",
    "preview": "vite preview --port 4173 --host '0.0.0.0'",
    "test:unit": "vitest --environment jsdom",
    "test:run": "vitest run --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "generate-typescript-fetch": "openapi-generator-cli generate -g typescript-fetch -i ../reference/test-open-api.yaml -o ./types/fetch"
  },
  "dependencies": {
    "@vueuse/core": "^9.0.0",
    "pinia": "^2.0.17",
    "sass": "^1.54.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@testing-library/vue": "^6.6.1",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.6.2",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "c8": "^7.12.0",
    "cypress": "^10.3.1",
    "jsdom": "^20.0.0",
    "msw": "^0.44.2",
    "msw-auto-mock": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.6",
    "typescript": "~4.7.4",
    "vite": "^3.0.3",
    "vitest": "^0.19.1",
    "vue-tsc": "^0.39.2",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
